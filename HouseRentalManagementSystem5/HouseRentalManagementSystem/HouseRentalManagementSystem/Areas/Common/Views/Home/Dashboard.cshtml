    `@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-md-4 col-xl-3">
        <div class="card bg-c-blue order-card">
            <div class="card-block">
                <h6 class="m-b-20">TOTAL TENANTS</h6>
                <h2 class="text-right"><i class="fas fa-users  f-left"></i><span>@ViewBag.TotalTenant</span></h2>
                <p class="m-b-0">Total Tetans<span class="f-right">@ViewBag.TotalTenant</span></p>
            </div>
        </div>
    </div>

    <div class="col-md-4 col-xl-3">
        <div class="card bg-c-green order-card">
            <div class="card-block">
                <h6 class="m-b-20">TOTAL BUILDINGS</h6>
                <h2 class="text-right"><i class="fas fa-building  f-left"></i><span>@ViewBag.TotalBuilding</span></h2>
                <p class="m-b-0">Total Building<span class="f-right">@ViewBag.TotalBuilding</span></p>
            </div>
        </div>
    </div>

    <div class="col-md-4 col-xl-3">
        <div class="card bg-c-yellow order-card">
            <div class="card-block">
                <h6 class="m-b-20">TOTAL APARTMENTS</h6>
                <h2 class="text-right"><i class="fas fa-home  f-left"></i><span>@ViewBag.TotalApartment</span></h2>
                <p class="m-b-0">Total Apartment<span class="f-right">@ViewBag.TotalApartment</span></p>
            </div>
        </div>
    </div>

    <div class="col-md-4 col-xl-3">
        <div class="card bg-c-pink order-card">
            <div class="card-block">
                <h6 class="m-b-20">TOTAL BOOKINGS</h6>
                <h2 class="text-right"><i class="fas fa-bookmark f-left"></i><span>@ViewBag.TotalBooking</span></h2>
                <p class="m-b-0">Total Booking<span class="f-right">@ViewBag.TotalBooking</span></p>
            </div>
        </div>
    </div>
</div>
